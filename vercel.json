{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/pages/payments/payment-detail/:payment_uuid",
      "destination": "/page/payments/payment-detail"
    },
    {
      "source": "/pages/sharing/:booking_uuid",
      "destination": "/sharing"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
